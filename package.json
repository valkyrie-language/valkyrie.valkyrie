{
    "name": "valkyrie-core",
    "private": true,
    "scripts": {
        "boot": "pnpm --filter @valkyrie-language/valkyrie-bootstrap bootstrap",
        "help": "pnpm --filter @valkyrie-language/valkyrie-document dev",
        "test": "cargo build",
        "fmt": "prettier --write \"**/*.{js,json}\" --ignore-path .prettierignore",
        "fmt:check": "prettier --check \"**/*.{js,json}\" --ignore-path .prettierignore",
        "push": "yarn test && git push",
        "reset": "git reset Head~ --soft",
        "u": "cargo upgrade --incompatible",
        "commit": "git-cz",
        "commitmsg": "commitlint -E GIT_PARAMS",
        "setup-commit": "npm install && npm install -g commitizen @commitlint/cli",
        "release:report": "node scripts/generate-release-report.js",
        "change-log": "node scripts/generate-release-report.js --changelog",
        "release:latest": "node scripts/generate-release-report.js $(date +v%Y.%m.%d)"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "commitizen": "^4.3.1",
        "commitlint": "^19.8.1",
        "cz-emoji": "^1.3.1",
        "prettier": "^3.0.0"
    },
    "config": {
        "commitizen": {
            "path": "cz-emoji"
        }
    },
    "packageManager": "pnpm@10.17.0"
}