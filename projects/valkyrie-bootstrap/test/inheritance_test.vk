namespace test;

class Base1 {
    micro get_name(self) -> String {
        return "Base1";
    }
}

class Base2 {
    micro get_type(self) -> String {
        return "Base2";
    }
}

class MyClass(Base1, Base2) {
    micro get_full_info(self) -> String {
        return self.get_name() + "-" + self.get_type();
    }
}

micro main() {
    let instance = new MyClass();
    console.log(instance.get_full_info());
}