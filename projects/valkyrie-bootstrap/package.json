{
    "name": "@valkyrie-language/valkyrie-bootstrap",
    "author": "Valkyrie Language Team",
    "version": "0.0.2",
    "description": "Valkyrie Language Bootstrap Compiler",
    "main": "bootstrap/index.js",
    "type": "module",
    "bin": {
        "valkyrie-bootstrap": "./bootstrap.js",
        "valkyrie": "./bootstrap.js"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "bootstrap": "node bootstrap.js bootstrap",
        "compile": "node bootstrap.js compile",
        "compile-dir": "node bootstrap.js compile-dir",
        "test": "node bootstrap.js test && pnpm fmt",
        "clean": "rimraf dist",
        "help": "node bootstrap.js help",
        "fmt": "prettier --write \"**/*.js\"",
        "fmt:check": "prettier --check \"**/*.js\" && prettier --check \"**/*.json\""
    },
    "keywords": [
        "valkyrie",
        "compiler",
        "bootstrap",
        "programming-language",
        "compiler-bootstrap",
        "javascript"
    ],
    "license": "ISC",
    "engines": {
        "node": ">=14.0.0"
    },
    "devDependencies": {}
}